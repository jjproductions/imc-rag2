# Core
API_KEY=local-key

# Qdrant
QDRANT_URL=http://localhost:6333
# QDRANT_COLLECTION=imc_corpus2
QDRANT_COLLECTION=board-policies

# Embeddings
EMBEDDING_MODEL=BAAI/bge-m3

# LLM (Ollama)
